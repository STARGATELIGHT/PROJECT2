pipeline{

    agent any

    stages{

        stage('Continues Download'){

            steps{
                git branch: 'main', url: 'https://github.com/STARGATELIGHT/PROJECT2.git'
            }
        }
        stage("Unit Test"){

            steps{
                sh "mvn test"
            }
        }
        stage("Integreted Test"){

            steps{
                sh 'mvn verify -DskipUnitTests'
            }
        }

    }
}